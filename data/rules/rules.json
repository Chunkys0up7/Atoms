    {
      "rule_id": "rule-low-credit",
      "name": "Low Credit Score Manual Review",
      "description": "Requires manual review for credit scores below 620",
      "priority": 9,
      "active": true,
      "created_at": "2025-12-23T21:16:32.663319",
      "updated_at": "2025-12-23T21:16:32.663319",
      "created_by": "system-migration",
      "version": 1,
      "condition": {
        "type": "AND",
        "rules": [
          {
            "field": "customer_data.credit_score",
            "operator": "LESS_THAN",
            "value": 620
          }
        ],
        "groups": null
      },
      "action": {
        "type": "INSERT_PHASE",
        "phase": {
          "id": "phase-manual-credit-review",
          "name": "Manual Credit Review",
          "description": "Enhanced review required due to credit risk",
          "position": "AFTER",
          "reference_phase": "phase-assessment",
          "modules": [
            "module-credit-analysis",
            "module-risk-assessment"
          ],
          "target_duration_days": 2
        },
        "modification": {
          "reason": "Credit score below threshold (620)",
          "criticality": "HIGH"
        }
      }
    },
    {
