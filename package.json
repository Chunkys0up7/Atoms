{
  "name": "gndp:-graph-native-documentation-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "set ESLINT_USE_FLAT_CONFIG=false && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:yaml": "yamllint atoms/ test_data/atoms/",
    "validate:atoms": "python scripts/validate_atoms.py atoms",
    "validate:atoms:verbose": "python scripts/validate_atoms.py atoms --verbose",
    "validate:test-atoms": "python scripts/validate_atoms.py test_data/atoms",
    "validate:all": "npm run validate:atoms && npm run validate:test-atoms",
    "validate": "npm run lint:yaml && npm run validate:all"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.34.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "d3": "^7.9.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^1.0.4"
  }
}
