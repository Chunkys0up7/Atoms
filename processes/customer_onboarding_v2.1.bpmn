<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_1" name="Customer Onboarding Process" processRef="Process_1" />
  </bpmn:collaboration>
  <bpmn:process id="customer_onboarding" name="Customer Onboarding" isExecutable="false">
    <bpmn:documentation>This process describes the end-to-end flow for onboarding a new customer, including KYC checks and account provisioning.</bpmn:documentation>
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_1" name="Relationship Manager">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CollectDocs</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_2" name="Compliance Officer">
        <bpmn:flowNodeRef>Task_ReviewKYC</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_3" name="System">
        <bpmn:flowNodeRef>Task_ProvisionAccount</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_1</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_1" name="Application Received">
      <bpmn:documentation>Process starts when a new application is received via the portal.</bpmn:documentation>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_CollectDocs" name="Collect ID Documents">
      <bpmn:documentation>The Relationship Manager collects physical or digital copies of ID documents (Passport, Utility Bill).</bpmn:documentation>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ReviewKYC" name="Review KYC">
      <bpmn:documentation>Compliance checks the documents against sanctions lists and verifies identity.</bpmn:documentation>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_ProvisionAccount" name="Provision Account">
      <bpmn:documentation>System automatically creates the account in the core banking ledger.</bpmn:documentation>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_1" name="Onboarding Complete">
      <bpmn:documentation>Customer is notified and account is active.</bpmn:documentation>
    </bpmn:endEvent>
  </bpmn:process>
</bpmn:definitions>
